Context()\r\n    \r\n    return (\r\n        <>{cart.length === 0 &&\r\n        <div className=\"row cart\">\r\n            <img src=\"https://i.gifer.com/XSNt.gif\" alt=\"No hay nada en tu carrito\"/>\r\n            <h3>No hay nada en tu carrito</h3>\r\n            <NavLink to=\"/\"><button type=\"button\" className=\"btn btn-productos btn-lg px-4 gap-3\" data-bs-container=\"body\" data-bs-toggle=\"popover\" data-bs-placement=\"top\" data-bs-content=\"Top popover\">Ver productos</button></NavLink>\r\n        </div>\r\n        }\r\n        {cart.length > 0 && \r\n        <div className=\"container\">\r\n            \r\n                <ListCartItems/>\r\n                <NavLink to=\"/checkout\">\r\n                    <button className=\"btn btn-productos btn-lg px-4 gap-3\">completar compra</button>\r\n                </NavLink>\r\n                \r\n        </div>\r\n        }\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Cart","import React from 'react'\r\n\r\nconst CartForm = ({formData, setFormData, inputs, handleSubmit}) => {\r\n     \r\n    function handleChange(e) {\r\n        setFormData(\r\n            {\r\n                ...formData,\r\n                [e.target.name]: e.target.value,\r\n            }\r\n        )\r\n    }\r\n    return (\r\n        <div className=\"form\">\r\n            <form\r\n            onSubmit={handleSubmit}\r\n            onChange={handleChange}>\r\n                {inputs.map((inp) => {\r\n                return(\r\n                    <div className=\"mt-3\">\r\n                        <label>{inp.label}</label>\r\n                        <input className=\"form-control\" id={inp.id} type={inp.type} name={inp.name} placeholder={inp.placeholder} value={inp.value} />\r\n                    </div>\r\n                )})\r\n                }\r\n                <div className=\"mt-5\">\r\n                    <button id=\"purchase\" className=\"btn btn-productos\">\r\n                        confirmar compra\r\n              